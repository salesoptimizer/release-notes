<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MainController</servlet-name>
  	<display-name>MainController</display-name>
  	<description></description>
  	<servlet-class>rncontroller.MainController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GGLService</servlet-name>
  	<display-name>GGLService</display-name>
  	<description></description>
  	<servlet-class>rnservices.GGLService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MainController</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GGLService</servlet-name>
  	<url-pattern>/doc</url-pattern>
  </servlet-mapping>
</web-app>
